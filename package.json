{
  "name": "FEC",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage",
    "watch": "-d --watch",
    "build": "webpack",
    "start": "node server/index.js && node database/seed.js ",
    "eslint": "eslint --ext .jsx .",
    "seed": "node database/seed.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Seattle-Navigators/tripadvisor_NearByAttractions_Jim.git"
  },
  "jest": {
    "verbose": true,
    "coverageDirectory": "specs/coverage",
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/client/dist/",
      "<rootDir>/client/src/style.css",
      "<rootDir>/specs/",
      "<rootDir>/.circleci/",
      "<rootDir>/node_modules/",
      "<rootDir>/.babelrc",
      "<rootDir>/.eslintrc.js",
      "<rootDir>/.gitignore",
      "<rootDir>/package.json",
      "<rootDir>/package-lock.json",
      "<rootDir>/README.md",
      "<rootDir>/webpack.config.js",
      "<rootDir>/__mocks__",
      "<rootDir>/database/seed.js",
      "<rootDir>/coverage"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/webpack/**",
      "<rootDir>/client/src/*"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "setupFilesAfterEnv": [
      "jest-enzyme"
    ],
    "testEnvironmentOptions": {
      "enzymeAdapter": "react16"
    },
    "clearMocks": true,
    "testURL": "http://127.0.0.1:3004"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Seattle-Navigators/tripadvisor_NearByAttractions_Jim/issues"
  },
  "homepage": "https://github.com/Seattle-Navigators/tripadvisor_NearByAttractions_Jim#readme",
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.6.0",
    "eslint": "7.2.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "jest": "^26.1.0",
    "npm": "^6.14.5",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.0.7"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.29",
    "@fortawesome/free-solid-svg-icons": "^5.13.1",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@react-google-maps/api": "^1.9.7",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "chance": "^1.1.6",
    "env": "0.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "express": "^4.17.1",
    "geolib": "^3.3.1",
    "jest-enzyme": "^7.1.2",
    "mongoose": "^5.9.19",
    "morgan": "^1.10.0",
    "node": "14.5.0",
    "nodemon": "^2.0.4",
    "path": "^0.12.7",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-modal": "^3.11.2",
    "react-rating": "^2.0.5",
    "react-test-renderer": "^16.13.1"
  }
}
